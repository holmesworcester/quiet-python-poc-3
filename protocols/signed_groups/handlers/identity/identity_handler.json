{
  "type": "identity",
  "projector": {
    "description": "Projects identity events for signed groups protocol",
    "func": "projector.project",
    "tests": [
      {
        "given": {
          "db": {"state": {}},
          "envelope": {
            "data": {
              "type": "identity",
              "pubkey": "test_pubkey_123",
              "privkey": "test_privkey_123",
              "name": "Alice"
            },
            "metadata": {}
          }
        },
        "then": {
          "db": {
            "state": {
              "identities": [
                {
                  "pubkey": "test_pubkey_123",
                  "privkey": "test_privkey_123",
                  "name": "Alice"
                }
              ]
            },
            "eventStore": [
              {
                "data": {
                  "type": "identity",
                  "pubkey": "test_pubkey_123",
                  "privkey": "test_privkey_123",
                  "name": "Alice"
                },
                "metadata": {}
              }
            ]
          }
        }
      }
    ]
  },
  "commands": {
    "create": {
      "description": "Creates an identity keypair",
      "func": "create.execute",
      "tests": [
        {
          "given": {"db": {}, "params": {"name": "Bob"}},
          "then": {
            "return": {
              "api_response": {
                "identityId": "*",
                "publicKey": "*",
                "name": "Bob"
              },
              "newEvents": [
                {"type": "identity", "pubkey": "*", "privkey": "*", "name": "Bob"}
              ]
            }
          }
        }
      ]
    }
  }
}