{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["envelope", "data", "metadata"],
  "properties": {
    "envelope": {
      "type": "string",
      "const": "encrypted"
    },
    "data": {
      "type": "string",
      "description": "Raw ciphertext as hex string"
    },
    "metadata": {
      "type": "object",
      "properties": {
        "algorithm": {
          "type": "string"
        },
        "event_id": {
          "type": "string"
        }
      }
    }
  }
}